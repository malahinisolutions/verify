!!!
%html
  %head
    %meta{:charset => "utf-8"}/
    %meta{:content => "width=device-width, initial-scale=1.0", :name => "viewport"}/
    %title= content_for?(:title) ? yield(:title) : "Verification System"
    %meta{:content => "#{content_for?(:description) ? yield(:description) : "Www"}", :name => "description"}/
    %link{:href => "//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css", :rel => "stylesheet"}/

    = stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    = javascript_include_tag 'identify.js'
    %script{:src => url}
    = csrf_meta_tags
  %body

    
:javascript
  $( document ).ready(function() {
  iamreal.verify3d('#{guid}', function (msg) { onComplete() }, '#{redirect_url}')
  });
